/* 问答样式 */
.msgList_answer{
  display: flex;
  margin-top: 30rpx;
}
.msgList_answer .articles{
  flex: 1;
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #666666;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 2; /** 显示的行数 **/
  overflow: hidden;  /** 隐藏超出的内容 **/
}
.msgList_answer .article{
  flex: 1;
  font-size: 28rpx;
  color: #666666;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 2; /** 显示的行数 **/
  overflow: hidden;  /** 隐藏超出的内容 **/
}
.msgList_answer image{
  width: 30rpx;
  height: 35rpx;
  margin-top: 10rpx;
}
.msgList_title image{
  width: 30rpx;
  height: 35rpx;
  margin-right: 20rpx;
}
