.content{
 margin-bottom: 98rpx;
}
.page-body {
  display: flex;
  flex: 1;
  flex-direction: column;
  background: #f8f8f8;
}
.color{
  width: 100%;
  background: #ffffff;
}
.doctorInfo{
  background: #FFFFFF;
}

.videoTitle{
  width: 92%;
  margin: auto;
  padding-top: 26rpx;
  padding-bottom: 50rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.videoTitle>view:nth-child(1){
  font-size: 34rpx;
  color: #333333;
  margin-bottom: 10rpx;
}
.videoTitle>view:nth-child(2)>text{
  color: #999999;
  font-size: 20rpx;
}
.videoTitle>view:nth-child(2)>text:nth-child(1){
  margin-right: 5rpx;
  font-size: 26rpx;
}
.videoTitle>view:nth-child(2)>text:nth-child(2){
  margin-right: 5rpx;
}
.videoTitle>view:nth-child(2)>text:nth-child(3){
  margin-right: 30rpx;
}
/* 专家信息 */
.InfoBox{
  width: 92%;
  margin:auto;
  display: flex;
  padding-top: 50rpx;
  padding-bottom: 50rpx;
  align-items: center;
}
.InfoBox>view:nth-child(1){
  width: 90rpx;
  height: 90rpx;
}
.InfoBox>view:nth-child(1) image{
  border-radius: 50%;
  width: 100%;
  height: 100%;
}
.InfoBox>view:nth-child(2){
  flex: 1;
  margin-left: 38rpx;
}
.InfoBox>view:nth-child(2)>view:nth-child(1)>text:nth-child(1){
  font-size: 32rpx;
  color: #333333;
  padding-right: 10rpx;
}
.InfoBox>view:nth-child(2)>view:nth-child(1)>text:nth-child(2){
  font-size: 28rpx;
  color: #666666;
}
.InfoBox>view:nth-child(2)>view:nth-child(2)>text:nth-child(1){
  font-size: 28rpx;
  color: #999999;
  padding-right: 20rpx;
}
.InfoBox>view:nth-child(2)>view:nth-child(2)>text:nth-child(2){
  font-size: 28rpx;
  color: #999999;
}
/* 视频内容 */
.title{
  width: 92%;
  margin: auto;
  font-size: 32rpx;
  color: #000000;
}
.title>view:nth-child(1){
  padding-bottom: 25rpx;
  font-size: 32rpx;
  color: #000000;
}
.title>view:nth-child(2){
  font-size: 28rpx;
  padding-bottom: 50rpx;
  color:#666666;
  text-indent:2em;
  line-height:1.8;
  letter-spacing:1rpx;

}
/* 相关内容 */
.recommended{
  width: 100%;
  background: #ffffff;
  margin-top: 20rpx;
}
.recommended-title{
  width: 92%;
  margin: auto;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  font-size: 32rpx;
  color: #000000;
  font-weight: 600
}
