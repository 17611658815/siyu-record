  margin-bottom: 10rpx;
}
/*自定义tabbar样式*/

.tabbar {
  position: fixed;
  display: -webkit-box;
  display: -moz-box;
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flexbox;
  display: flex;
  bottom: 0;
  left: 0;
  right: 0rpx;
  background-color: #fff;
  height: 96rpx;
  width: 100%;
  border-top: 2rpx solid #ededed;
}

.barBtn {
  flex: 1;
}

.btnIcon {
  width: 46rpx;
  height: 46rpx;
  margin: 8rpx auto;
}

.btnName {
  font-size: 20rpx;
  color: #929294;
  text-align: center;
}

.active {
  color: #6ea8f7;
}




