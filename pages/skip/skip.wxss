  background: #6ea8f7;
  color: #ffffff;
  font-size: 34rpx;
  margin-top: 50rpx;
  border-radius: 44px 44px 44px 44px;
  border: none;
}
.nextSteps{
  width: 80%;
  background: #BCBCBC;
  color: #ffffff;
  font-size: 34rpx;
  margin-top: 50rpx;
